<?xml version="1.0" encoding="UTF-8"?>
<protocol name="nemo_sound">
	<interface name="nemo_sound" version="1">
		<event name="enter">
			<arg name="surface" type="object" interface="wl_surface"/>
			<arg name="device" type="string"/>
		</event>

		<event name="leave">
			<arg name="surface" type="object" interface="wl_surface"/>
			<arg name="device" type="string"/>
		</event>
	</interface>
</protocol>
